#!/usr/bin/env-bash

cd /home/elninosec16/Documents/TF_BootCamp/tf-bootcamp-repo/workspace

rm -f '/home/elninosec16/Documents/TF_BootCamp/tf-bootcamp-repo/workspace/awscliv2.zip'
rm -rf '/home/elninosec16/Documents/TF_BootCamp/tf-bootcamp-repo/workspace/aws'

cd /home/elninosec16/Documents/TF_BootCamp/tf-bootcamp-repo/workspace
    curl "https://awscli.amazonaws.com/awscli-exe-linux-x86_64.zip" -o "awscliv2.zip"
    unzip awscliv2.zip
    sudo ./aws/install
    cd $THEIA_WORKSPACE_ROOT

aws sts get-caller-identity

cd $PROJECT_ROOT

